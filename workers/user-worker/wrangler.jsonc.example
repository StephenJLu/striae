{
  "name": "striae-users",
  "account_id": "YOUR_ACCOUNT_ID",
  "main": "src/user-worker.js",
  "compatibility_date": "2023-03-14",
  "compatibility_flags": [
    "nodejs_compat"
  ], 
  
  "observability": { 
	"enabled": true
  },

   "kv_namespaces": [
    {
      "binding": "USER_DB",
      "id": "USER_DB"
    }
  ],

  "services": [
    {
      "binding": "KEYS_WORKER",
      "service": "striae-keys"
    }
  ],

  "routes": [
    {
      "pattern": "USER_WORKER_CUSTOM_DOMAIN",
      "custom_domain": true
    }
  ],

  "placement": { "mode": "smart" }
}